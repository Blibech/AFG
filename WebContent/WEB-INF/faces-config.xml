			<from-action>#{userBean.logout}</from-action>
			<from-outcome>logout</from-outcome>
			<to-view-id>/pages/connect.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>
	
	<navigation-rule>
		<from-view-id>/pages/demandes.xhtml</from-view-id>
		<navigation-case>
			<from-action>#{userBean.logout}</from-action>
			<from-outcome>logout</from-outcome>
			<to-view-id>/pages/connect.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>
	
	<navigation-rule>
		<from-view-id>/pages/maintenance.xhtml</from-view-id>
		<navigation-case>
			<from-action>#{userBean.logout}</from-action>
			<from-outcome>logout</from-outcome>
			<to-view-id>/pages/connect.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>

	<navigation-rule>
		<from-view-id>/pages/utilisateurs.xhtml</from-view-id>
		<navigation-case>
			<from-action>#{userBean.logout}</from-action>
			<from-outcome>logout</from-outcome>
			<to-view-id>/pages/connect.xhtml</to-view-id>
		</navigation-case>
		<navigation-case>
			<from-action>#{userBean.saveNouveau}</from-action>
			<from-outcome>ADDED</from-outcome>
			<to-view-id>/pages/relode.xhtml</to-view-id>
		</navigation-case>
		<navigation-case>
			<from-action>#{userBean.edit}</from-action>
			<from-outcome>EDITED</from-outcome>
			<to-view-id>/pages/relode.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>

	<navigation-rule>
		<from-view-id>/pages/header.xhtml</from-view-id>
		<navigation-case>
			<from-action>#{navigationRulesBean.utilisateurs}</from-action>
			<from-outcome>utilisateurs</from-outcome>
			<to-view-id>/pages/utilisateurs.xhtml</to-view-id>
		</navigation-case>
		<navigation-case>
			<from-action>#{navigationRulesBean.home}</from-action>
			<from-outcome>home</from-outcome>
			<to-view-id>/pages/home.xhtml</to-view-id>
		</navigation-case>
		<navigation-case>
			<from-action>#{navigationRulesBean.produits}</from-action>
			<from-outcome>produits</from-outcome>
			<to-view-id>/pages/produits.xhtml</to-view-id>
		</navigation-case>
		<navigation-case>
			<from-action>#{navigationRulesBean.fournisseurs}</from-action>
			<from-outcome>fournisseurs</from-outcome>
			<to-view-id>/pages/fournisseurs.xhtml</to-view-id>
		</navigation-case>
		<navigation-case>
			<from-action>#{navigationRulesBean.outillages}</from-action>
			<from-outcome>outillages</from-outcome>
			<to-view-id>/pages/outillages.xhtml</to-view-id>
		</navigation-case>
		<navigation-case>
			<from-action>#{navigationRulesBean.acceuil}</from-action>
			<from-outcome>acceuil</from-outcome>
			<to-view-id>/pages/acceuil.xhtml</to-view-id>
