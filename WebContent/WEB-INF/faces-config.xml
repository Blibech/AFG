<?xml version="1.0" encoding="UTF-8"?>
<faces-config version="2.0" xmlns="http://java.sun.com/xml/ns/javaee"
	xmlns:xi="http://www.w3.org/2001/XInclude" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-facesconfig_2_0.xsd">
	<navigation-rule>
		<from-view-id>/pages/connect.xhtml</from-view-id>
		<navigation-case>
			<from-action>#{userBean.connecter}</from-action>
			<from-outcome>accepted</from-outcome>
			<to-view-id>/pages/acceuil.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>

	<navigation-rule>
		<from-view-id>/pages/acceuil.xhtml</from-view-id>
		<navigation-case>
			<from-action>#{navigationRulesBean.utilisateurs}</from-action>
			<from-outcome>utilisateurs</from-outcome>
			<to-view-id>/pages/utilisateurs.xhtml</to-view-id>
		</navigation-case>
		<navigation-case>
			<from-action>#{navigationRulesBean.home}</from-action>
			<from-outcome>home</from-outcome>
			<to-view-id>/pages/home.xhtml</to-view-id>
		</navigation-case>
		<navigation-case>
			<from-action>#{navigationRulesBean.produits}</from-action>
			<from-outcome>produits</from-outcome>
			<to-view-id>/pages/produits.xhtml</to-view-id>
		</navigation-case>
		<navigation-case>
			<from-action>#{navigationRulesBean.fournisseurs}</from-action>
			<from-outcome>fournisseurs</from-outcome>
			<to-view-id>/pages/fournisseurs.xhtml</to-view-id>
		</navigation-case>
		<navigation-case>
			<from-action>#{navigationRulesBean.outillages}</from-action>
			<from-outcome>outillages</from-outcome>
			<to-view-id>/pages/outillages.xhtml</to-view-id>
		</navigation-case>
		<navigation-case>
			<from-action>#{navigationRulesBean.acceuil}</from-action>
			<from-outcome>acceuil</from-outcome>
			<to-view-id>/pages/acceuil.xhtml</to-view-id>
		</navigation-case>
		<navigation-case>
			<from-action>#{navigationRulesBean.demandes}</from-action>
			<from-outcome>demandes</from-outcome>
			<to-view-id>/pages/demandes.xhtml</to-view-id>
		</navigation-case>
		<navigation-case>
			<from-action>#{navigationRulesBean.maintenance}</from-action>
			<from-outcome>maintenance</from-outcome>
			<to-view-id>/pages/maintenance.xhtml</to-view-id>
		</navigation-case>
		<navigation-case>
			<from-action>#{userBean.logout}</from-action>
			<from-outcome>logout</from-outcome>
			<to-view-id>/pages/connect.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>

	<navigation-rule>
		<from-view-id>/pages/home.xhtml</from-view-id>
		<navigation-case>
			<from-action>#{userBean.logout}</from-action>
			<from-outcome>logout</from-outcome>
			<to-view-id>/pages/connect.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>
	
	<navigation-rule>
		<from-view-id>/pages/produits.xhtml</from-view-id>
		<navigation-case>
			<from-action>#{userBean.logout}</from-action>
			<from-outcome>logout</from-outcome>
			<to-view-id>/pages/connect.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>
	
	<navigation-rule>
		<from-view-id>/pages/outillages.xhtml</from-view-id>
		<navigation-case>
			<from-action>#{userBean.logout}</from-action>
			<from-outcome>logout</from-outcome>
			<to-view-id>/pages/connect.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>
	
	<navigation-rule>
